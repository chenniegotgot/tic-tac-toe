{
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "trailing-comma": false,
    "array-type": [true, "generic"],
    "quotemark": [
      true,
      "single"
    ],
    "no-console": [
      true,
      "log",
      "debug",
      "info",
      "time",
      "timeEnd",
      "trace"
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case",
      "allow-leading-underscore"
    ],
    "typedef-whitespace": false
  }
}
